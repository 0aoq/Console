{
    "manifest_version": 2,
    "name": "Terminal",
    "version": "1.0",

    "description": "yes",

    "background": {
        "scripts": [
            "console.js"
        ]
    },

    "permissions": [
        "activeTab"
    ],

    "browser_action": {
        "default_title": "Terminal",
        "default_popup": "console.html"
    }
}